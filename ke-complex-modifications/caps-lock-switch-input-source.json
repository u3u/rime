{
  "rules": [
    {
      "description": "Pressing caps_lock sends f17, pressing fn + caps_lock sends caps_lock",
      "manipulators": [
        {
          "from": { "key_code": "caps_lock" },
          "to": [{ "key_code": "f17" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "caps_lock", "modifiers": { "mandatory": ["fn"] } },
          "to": [{ "key_code": "caps_lock" }],
          "type": "basic"
        }
      ]
    }
  ],
  "title": "Capslock switch input source"
}
